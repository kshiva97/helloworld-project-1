pipeline{
agent{label 'master'}

tools{
    jdk 'java'
    maven 'maven'
}

stages{
    stage('gitclone'){
        steps{
        git branch: 'main',
        url: 'https://github.com/chinni4321/helloworld.git'
            
       
        }
    }

    stage('maven build'){
        steps{
            sh 'mvn clean package'
            
            
        }
    }
   
}
}
